{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqliteConnection": "Data Source=movies.db"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Movies API - Health Status",
        "Uri": "/health"
      }
    ],
    "EvaluationTimeInSeconds": 5
  }
}
